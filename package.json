  "dependencies": {
    "bluebird": "^3.7.2",
    "fast-xml-parser": "^3.17.4"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "libxmljs": "^0.19.7",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^8.2.0",
    "moment": "^2.29.1",
    "nunjucks": "^3.2.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "shelljs": "^0.8.4",
    "sinon": "^9.2.0",
    "spectron": "8.0.0"
  },
  "nyc": {
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "exclude": [
