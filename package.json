{
  "name": "MMM-RBB-Weather",
  "version": "1.0.0",
  "description": "MagicMirror module to display RBB weather data.",
  "author": "Nikolai Keist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkl-kast/MMM-RBB-Weather.git"
  },
  "main": "MMM-RBB-Weather.js",
  "scripts": {
    "test": "nyc mocha \"test/unit/**/*.js\"",
    "test-int": "mocha \"test/int/**/*.js\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "bluebird": "latest",
    "fast-xml-parser": "latest"
  },
  "devDependencies": {
    "coveralls": "latest",
    "decache": "latest",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdom": "latest",
    "libxmljs": "latest",
    "mocha": "latest",
    "moment": "latest",
    "nyc": "latest",
    "proxyquire": "latest",
    "sinon": "latest"
  },
  "nyc": {
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "exclude": [
      "test",
      "coverage",
      "Logger.js"
    ],
    "cache": true,
    "all": true
  }
}
